{
  "name": "ramesh-dada/customtheme",
  "description": "1Dot Custom Theme",
  "keywords": [
    "flarum"
  ],
  "type": "flarum-extension",
  "license": "MIT",
  "require": {
    "flarum/core": "^1.0.0",
    "afrux/flarum-theme-base": "^0.1.0"
  },
  "require-dev": {
    "sycho/flarum-uikit": "0.1.x-dev",
    "afrux/flarum-theme-base": "0.1.x-dev"
  },
  "authors": [
    {
      "name": "Ramesh Dada",
      "role": "Developer"
    }
  ],
  "support": {
    "forum": "https://discuss.flarum.org/u/1Dot"
  },
  "autoload": {
    "psr-4": {
      "rameshdada\\customtheme\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "rameshdada\\Asirem\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    },
    "flarum-extension": {
      "title": "Asirem",
      "category": "theme",
      "optional-dependencies": [
          "flarum/tags",
          "flarum/approval",
          "sycho/flarum-advanced-extension-categories"
        ],
      "icon": {
        "image": "icon.svg"
      }
    }
  }
}
