{
  "name": "ramesh-dada/customtheme",
  "description": "1Dot Custom Theme",
  "keywords": [
    "flarum"
  ],
  "type": "flarum-extension",
  "license": "MIT",
  "require": {
    "flarum/core": "^1.0"
  },
  "authors": [
    {
      "name": "Ramesh Dada",
      "role": "Developer"
    }
  ],
  "support": {
    "forum": "https://discuss.flarum.org/u/1Dot"
  },
  "autoload": {
    "psr-4": {
      "rameshdada\\customtheme\\": "src/"
    }
  },

  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    },
    "flarum-extension": {
      "title": "Asirem",
      "category": "theme",
      "optional-dependencies": [
          "flarum/tags",
          "flarum/approval"
        ],
      "icon": {
        "image": "icon.svg"
      }
    }
  }
}
